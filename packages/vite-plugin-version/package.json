  "keywords": [
    "vite",
    "plugin",
    "version",
    "build",
    "inject"
  ],
  "homepage": "https://github.com/crescendolab-open/vite-plugin-version",
  "repository": {
    "type": "git",
    "url": "https://github.com/crescendolab-open/vite-plugin-version.git"
  },
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
